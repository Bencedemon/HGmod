  "Mark.8heartII.Horse.00": "Silver Boat",
  "Mark.8heartII.Horse.01": "Unique Month",
  "Mark.8heartII.Horse.02": "Tokaj Tejo",
  "Mark.8heartII.Horse.03": "Oguri Hat",
  "Mark.8heartII.Horse.04": "Tequila",
  "Mark.8heartII.Horse.05": "Turf Miracle",
